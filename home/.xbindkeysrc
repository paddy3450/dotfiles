# Increase volume
#"pactl set-sink-volume 0 +5"
"pamixer -i 5"
   XF86AudioRaiseVolume

# Decrease volume
#"pactl set-sink-volume 0 -5"
"pamixer -d 5"
   XF86AudioLowerVolume

# Mute volume
#"pactl set-sink-mute 0 toggle"
"pamixer -t"
   XF86AudioMute

# Mute volume Alt version
#"pactl set-sink-mute 0 toggle"
#"pamixer -t"
   #Control+Shift+Mod4+Down

# Mute mic
#"pactl set-source-output-mute 0 toggle"
"amixer set Capture toggle"
   XF86AudioMicMute
 
# Toggle mic mute
"amixer set Capture toggle"
   Control+Mod4+m

# Increase backlight
#"xbacklight -inc 10"
"brightnessctl s +10%"
   XF86MonBrightnessUp

# Decrease backlight
#"xbacklight -dec 10"
"brightnessctl s 10%-"
   XF86MonBrightnessDown

